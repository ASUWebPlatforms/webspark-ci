        <div class="card-button">
            <a
                href="#"
                class="btn btn-gold"
                data-ga="Button text link here"
                data-ga-name="onclick"
                data-ga-event="link"
                data-ga-action="click"
                data-ga-type="internal link"
                data-ga-region="main content"
                data-ga-section="card default title"
                >
                Button link here
                </a>
        </div>
    </div>
    <div class="card-link">
        <a
            href="#"
            class=""
            data-ga="Text link here"
            data-ga-name="onclick"
            data-ga-event="link"
            data-ga-action="click"
            data-ga-type="internal link"
            data-ga-region="main content"
            data-ga-section="card default title"
            >
