{#
/**
 * @file
 * Template for a block.
 */
#}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    {% block label %}
      <h2{{ title_attributes }}>{{ label }}</h2>
    {% endblock %}
  {% endif %}
  {{ title_suffix }}
  {% include '@renovation/divider/divider.twig' with {
    divider_type: content.field_divider_type[0]['#markup']
  }%}
</div>
